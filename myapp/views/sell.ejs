<h1> Post your property for Sale </h1>

    <form method="post" enctype="multipart/form-data"> 

        <label for="address"> Street Address:</label>
        <input type="text" id="address" name="address" placeholder="1600, Holloway Avenue" required />
    <br><br>
        <label for="city">City:</label>
        <input type="text" id="city" name="city" placeholder="San Francisco" required />
    <br><br>
        <label for="city">State:</label>
        <input type="text" id="state" name="state" placeholder="CA" required />
    <br><br>
        <label for="city">Zip:</label>
        <input type="text" pattern="[0-9]{5}"  id="zip" name="zip" placeholder="94132" required />
    <br><br>
        <label for="city">Property Image:</label>
        <input type="file" id="myimage" name="myimage" required /> <br/>
        <input type="submit" name="submit" value="Submit" />
    </form>  

